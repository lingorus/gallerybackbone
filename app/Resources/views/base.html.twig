<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('bundles/app/css/gallery.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}


        {% block javascripts %}
            <script
                    src="{{ asset('assets/vendor/jquery/dist/jquery.js') }}"></script>
            <script
                    src="{{ asset('assets/vendor/underscore/underscore.js') }}"></script>
            <script
                  src="{{ asset('assets/vendor/backbone/backbone.js') }}"></script>
            <script
                    src="{{ asset('assets/vendor/backbone.radio/build/backbone.radio.js') }}"></script>
            <script
                  src="{{ asset('assets/vendor/backbone.marionette/lib/backbone.marionette.js') }}"></script>

            {% javascripts output="/js/instant.js"
            '@AppBundle/Resources/public/js/models.coffee'
            '@AppBundle/Resources/public/js/views.coffee'
            '@AppBundle/Resources/public/js/application.coffee'
            '@AppBundle/Resources/public/js/router.coffee'
            '@AppBundle/Resources/public/js/app.coffee'
            filter="coffee"
            %}
                <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
